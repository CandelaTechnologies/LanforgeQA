{
   "ct_ap_auto_001_scripts":{
       "Notes":[
           "This json file describes tests to be run by LANforge system",
           "When doing a create_chamberview.py --create_scenario <name> ",
           "has no correlation to the --instance_name , instance name is used ",
           "as a unique identifier for tha chamber-view test run"
       ]  
   },
   "test_suites":{
       "ct_ap_auto_2g_5g_001":{
           "CC_AP_AUTO_DUT_2G_5G":{
               "enabled":"TRUE",
               "load_db":"skip",
               "command":"create_chamberview_dut.py",
               "args":"",
               "args_list":[
                   " --lfmgr LF_MGR_IP --port LF_MGR_PORT --dut_name USE_DUT_NAME",
                   " --ssid 'ssid_idx=0 ssid=SSID_USED security=SECURITY_USED password=SSID_PW_USED bssid=BSSID_TO_USE'",
                   " --ssid 'ssid_idx=1 ssid=SSID_USED security=SECURITY_USED password=SSID_PW_USED bssid=BSSID_TO_USE'",
                   " --sw_version DUT_SW --hw_version DUT_HW --serial_num DUT_SERIAL --model_num USE_DUT_NAME"
               ]
           },
           "CC_AP_AUTO_2G_5G":{
               "enabled":"TRUE",
               "load_db":"skip",
               "command":"create_chamberview.py",
               "args":"",
               "args_list":[
                   " --lfmgr LF_MGR_IP --port LF_MGR_PORT --delete_scenario",
                   " --create_scenario AP_AUTO_2G_5G ",
                   " --raw_line \"profile_link 1.1 STA-AUTO 6 'DUT: axe16000 Radio-1' NA AUTO,AUTO -1 NA\" ",                   
                   " --raw_line \"profile_link 1.1 STA-AUTO 6 'DUT: axe16000 Radio-2' NA AUTO,AUTO -1 NA\" ",
                   " --raw_line \"profile_link 1.1 upstream-dhcp 1 NA NA eth2,AUTO -1 NA\" "                  
               ]
           },
           "AP_AUTO_2G_5G":{
               "enabled":"TRUE",
               "timeout":"600",
               "iterations":"1",
               "load_db":"skip",
               "command":"lf_ap_auto_test.py",
               "args":"",
               "args_list":[
                  " --mgr LF_MGR_IP --port LF_MGR_PORT --lf_user lanforge --lf_password lanforge",
                  " --instance_name AP_AUTO_2G_5G",
                  " --lf_user lanforge",
                  " --lf_password lanforge",
                  " --config_name test_con",
                  " --upstream UPSTREAM_PORT",
                  " --dut2_0 'axe16000 lanforge C8:7F:54:14:42:2C (1)'",
                  " --dut5_0 'axe16000 lanforge C8:7F:54:14:42:30 (2)'",
                  " --max_stations_2 6",
                  " --max_stations_5 6",
                  " --max_stations_6 0",
                  " --max_stations_dual 1",
                  " --radio2 '1.1.4 wiphy0' --radio2 '1.1.6 wiphy2' --radio2 '1.1.7 wiphy4' --radio2 '1.1.8 wiphy5' --radio2 '1.1.9 wiphy6' --radio2 '1.1.10 wiphy7'",
                  " --radio5 '1.1.6 wiphy2' --radio5 '1.1.7 wiphy4' --radio5 1.1.wiphy5 --radio5 1.1.wiphy6 --radio5 1.1.wiphy7",
                  " --set 'basic_cx' 1",
                  " --set 'band_steering' 0",
                  " --set 'capacity' 0",
                  " --set 'channel_switch' 0",
                  " --set 'long_term' 0",
                  " --set 'mix_stability' 0",
                  " --set 'tput_single_sta' 0",
                  " --set 'tput_multi_sta' 0",
                  " --set 'tput_multi_band' 0",
                  " --set 'skip_2' 0",
                  " --set 'skip_5' 0",
                  " --set 'skip_5b' 1",
                  " --set 'skip_6' 1",
                  " --verbosity 11",
                  " --raw_line 'cx_psk_thresh: 500'",
                  " --pull_report",
                  " --local_lf_report_dir REPORT_PATH",
                  " --test_tag 'AP_AUTO_001'",
                  " --test_rig LANforge",
                  " --set DUT_NAME axe16000"
               ]
           },
           "lf_qa":{
               "enabled":"TRUE",
               "timeout":"600",
               "load_db":"skip",
               "command":"./tools/lf_qa.py",
               "args":"",
               "args_list":[
                   " --server TEST_SERVER --path REPORT_PATH --store --png --database DATABASE_SQLITE"
               ]
           },
           "lf_inspect":{
               "enabled":"TRUE",
               "timeout":"600",
               "load_db":"skip",
               "command":"./tools/lf_inspect.py",
               "args":"",
               "args_list":[
                   " --path REPORT_PATH --database DATABASE_SQLITE --db_index 0,1"
               ]
           }
       }
   }
}
